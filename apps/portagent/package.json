{
  "name": "@agentxjs/portagent",
  "version": "1.4.0",
  "description": "Portagent - AgentX Portal Application",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deepractice/AgentX.git"
  },
  "homepage": "https://github.com/Deepractice/AgentX",
  "type": "module",
  "bin": {
    "portagent": "./bin/portagent.js"
  },
  "main": "./dist/server/index.js",
  "files": [
    "dist",
    "bin",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "concurrently \"bun dev:server\" \"bun dev:client\"",
    "dev:server": "bun --watch src/server/main.ts",
    "dev:client": "bun dev.ts",
    "build": "bun run build.ts",
    "start": "node dist/cli/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentxjs/runtime": "workspace:*",
    "@agentxjs/ui": "workspace:*",
    "@hono/node-server": "^1.13.7",
    "agentxjs": "workspace:*",
    "commander": "^12.1.0",
    "dotenv": "^16.6.1",
    "hono": "^4.6.12",
    "jose": "^5.9.6",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.1.0",
    "postcss": "8.5.6",
    "postcss-cli": "11.0.1",
    "typescript": "^5.9.3"
  }
}
