{
  "name": "agentxjs",
  "version": "1.4.0",
  "description": "Unified API for AI Agents - Server and Browser",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deepractice/AgentX.git"
  },
  "homepage": "https://github.com/Deepractice/AgentX",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "clean": "rm -rf dist test-results",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@agentxjs/common": "workspace:*",
    "@agentxjs/network": "workspace:*",
    "@agentxjs/types": "workspace:*",
    "reconnecting-websocket": "^4.4.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@agentxjs/runtime": "workspace:*",
    "@types/node": "^24.9.2",
    "@types/ws": "^8.5.13",
    "bun-dts": "0.1.70",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
